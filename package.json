{
  "name": "note-synchronizer",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./gmail.threads.data attachments/* processed/*",
    "prestart": "mkdir -p attachments processed && npm run format:js && clear",
    "start": "ts-node index.ts",
    "start-single": "echo ./node_modules/.bin/ts-node index.ts 123",
    "process": "ts-node process.ts",
    "process-single": "echo ./node_modules/.bin/ts-node process.ts 123",
    "playground": "ts-node playground.test.ts",
    "test": "ts-node index.ts && ts-node process.ts",
    "format": "npm run format:js && npm run format:data",
    "format:data": "prettier --parser json --write **/*.data",
    "format:js": "prettier --write **/**/*.js **/**/*.ts *.ts *.md && prettier --parser json --write *.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/synle/note-synchronizer.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/synle/note-synchronizer/issues"
  },
  "homepage": "https://github.com/synle/note-synchronizer#readme",
  "dependencies": {
    "@mozilla/readability": "^0.3.0",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "googleapis": "^39.2.0",
    "js-base64": "^3.4.5",
    "jsdom": "^16.4.0",
    "moment": "^2.27.0",
    "sequelize-typescript-decorators": "^2.1.7",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.27",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
